{
    "family": "ec2-task",
    "containerDefinitions": [
        {
            "name": "ec2-task-container",
            "image": "DOCKER_IMAGE_URI", 
            "cpu": 512,
            "memory": 2048,
            "memoryReservation": 2048,            
            "portMappings": [
                {
                    "containerPort": 80, 
                    "protocol": "tcp"
                }
            ], 
            "essential": true
        }
    ], 
    "requiresCompatibilities": [
        "EC2"
    ]
}